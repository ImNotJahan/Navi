~ remainder has to be loaded as well as divisible uses it
(load "navi/math.navi" "divisible remainder")

(set (fizzbuzz x)
	(for (set i 1) (<= i x) (++ i)
		(let ((fizz (divisible i 3)) (buzz (divisible i 5)))
			(if (and fizz buzz) 
				(sayline "fizzbuzz")
				(if fizz 
					(sayline "fizz")
					(if buzz
						(sayline "buzz")
						(sayline i)))))))