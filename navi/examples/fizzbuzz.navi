(set (fizzbuzz x)
	(set (fizzbuzz_helper x i)
		(if (> i x)
			(error "OK")
			(let ((fizz (= (remainder i 3) 0)) (buzz (= (remainder i 5) 0)))
				(if (and fizz buzz) 
					(say "fizzbuzz")
					(if fizz 
						(say "fizz")
						(if buzz
							(say "buzz")
							(say i))))
				(fizzbuzz_helper x (+ 1 i)))))
	(fizzbuzz_helper x 1))