(set (get list index) 
	(if (null? list)
		null
		(if (not= 0 index)
			(get (tail list) (- index 1))
			(head list))))

(set (in x list)
	(if (null? list)
		false
		(if (= (head list) x)
			true
			(in x (tail list)))))
	
(set (count n) (if (= n 0) 0 (+ 1 (count (- n 1)))))

(set (break) (error "EMPTY"))

