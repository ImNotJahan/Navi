(set (sayline a) (say a) (newline))

(set (// a b) (int (/ a b)))

(set (range start end)
	(let ((result null))
		(if (< start end)
			(for (set i start) (<= i end) (++ i) do
				(push i to result))
			(for (set i start) (>= i end) (-- i) do
				(push i to result)))
		(reverse result)))
		
(set (and p1 p2)
	(if p1 then_return
		p2 otherwise_return
		false))

(set (or p1 p2)
	(if p1 then_return true otherwise_return p2))